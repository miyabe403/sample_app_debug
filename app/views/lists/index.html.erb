<div class="list-wrap">
<% @lists.each do |list| %>
  <div class="list-box">
    <% if list.image.attached? %>
      <%= image_tag list.image, size: "200x200" %>
    <% else %>
      <%= image_tag 'no_image', size: "200x200" %>
    <% end %>
    <div class="list-comment">
      <span class="list-body"><%= list.body %></span>
      <span class="list-title"><%=link_to list.title, list_path(list.id) %></span> <!-- list.idの""を外す-->
      <!--<span class="list-title">%=link_to list.title, list_path("list.id") %></span>-->　<!-- エラー行 -->
    </div>
  </div>
<% end %>
</div>